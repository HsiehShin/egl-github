        NA000W01.CD_TRANS_CICS = NA000I-TRANS-CICS-CALL;
        move NA000I-DESTINO-CALL to NA000I-DESTINO withV60Compat;
        set NAEXTW01 empty;
        NAEXTW01.NAEXTI-PARAMETRO = NA000W01.NA000I-PARAMETRO;
        NAEXTI-TRAN-CODE = NA000W01.CD_TRANS_CICS;
        sysVar.transferName = NA000W01.CD_TRANS_CICS;
        NAEXTI-VAGO = ", ,";
        NAEXTI-LL = 155;
        transfer to transaction sysVar.transferName passing NAEXTW01 {isExternal = yes}; /* aplicacoes externas (nao CSP)*/
      else
        if (NA000I-TRANS-CICS-CALL == "SH02")
          transfer to program "NAV0MA0" passing NA000W01; /* transfere para navegador*/
        else
          if (NA000I-TRANS-CICS-CALL == "XSPS")
            sysVar.transferName = NA000I-APL-CALL;
            transfer to program sysVar.transferName passing NA000W01; /* transferencia via DXFR*/
          else
            sysVar.transferName = NA000I-TRANS-CICS-CALL;
            transfer to transaction sysVar.transferName passing NA000W01; /* transferencia via XFER*/
          end
        end
      end
    end
