// trata sqlcodes do add
Function NA000S-ADD()
  converseLib.validationFailed(0);
  if (sysVar.sqlData.sqlcode != 0)
    NA000I-EZESQCOD = sysVar.sqlData.sqlcode;
    NA000I-EZESQRRM = sysVar.sqlData.sqlerrmc;
    NA000I-APLICACAO = NA000W01.NA000I-APL-CHAMADA;
    if (sysVar.sqlData.sqlcode == -803)
    /* CALL EZEROLLB;*/
      converseLib.validationFailed(25); /* inclusao de existente*/
    else
      if (sysVar.sqlData.sqlcode == -530) /* integridade referenciada*/
        VGVar.handleSysLibraryErrors = 0;
        sysLib.rollback();
        SYSIBM_FOREIGNKEYS.RELNAME = NA000I-EZESQRRM;
        NA000P-INQ-FK();
        /**/
  /* MOVE NA000I-EZESQRRM TO SYSFOREIGNKEYS.RELNAME;*/
  /* NA000P-INQ-VW7FK();*/
        if (sysVar.sqlData.sqlcode == 0)
          NA000I-COLNAME = SYSIBM_FOREIGNKEYS.COLNAME;
          NA000I-TBNAME = SYSIBM_FOREIGNKEYS.TBNAME;
          /**/
  /* MOVE SYSFOREIGNKEYS.COLNAME NA000I-COLNAME;*/
  /* MOVE SYSFOREIGNKEYS.TBNAME NA000I-TBNAME;*/
          call "NAERSA0" (NA000I-APLICACAO, NA000I-PROCESSO, NA000I-EZESQCOD, NA000I-EZESQRRM, NA000I-COLNAME, NA000I-TBNAME);
          converseLib.validationFailed(149); /* erro foreing key*/
        else
          converseLib.validationFailed(1); /* problema ...*/
        end
      else
        VGVar.handleSysLibraryErrors = 0;
        sysLib.rollback();
        call "NAERRA0" (NA000I-APLICACAO, NA000I-PROCESSO, NA000I-EZESQCOD, NA000I-EZESQRRM);
        NA000W01.NA000I-PARAMETRO = " ";
        sysVar.transferName = NA000I-APLICACAO;
        transfer to program sysVar.transferName passing NA000W01;
      end
    end
  end
end // end NA000S-ADD

