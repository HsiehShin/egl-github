// Atualiza Unidade Federacao
Function T170UP-EXE-ATUAL()
  T170UP-EXE-CRITICA();
  if (converseVar.validationMsgNum != 0)
    return;
  end
  T170UI-IND = 0;
  while (T170UI-IND < 13)
    T170UI-IND = T170UI-IND + 1;
    if (T170UM01.T170UI-ACAO[T170UI-IND] != " ")
      SRHXVW81_UNID_FED.SI_UF = T170UM01.T170UI-SI-UF[T170UI-IND];
      if (T170UM01.T170UI-ACAO[T170UI-IND] == "E") /* exclusao*/
        T170UP-SQX-DELECAO(); /* deleta linha*/
        if (converseVar.validationMsgNum != 0)
          VGVar.handleSysLibraryErrors = 0;
          sysLib.rollback();
          set T170UM01.T170UI-ACAO[T170UI-IND] bold;
          return;
        end
      else
        SRHXVW81_UNID_FED.NO_UF = T170UM01.T170UI-NO-UF[T170UI-IND];
        if (T170UM01.T170UI-ACAO[T170UI-IND] == "A") /* alteracao*/
          T170UP-SQX-ALTERA(); /* altera a linha*/
          if (converseVar.validationMsgNum != 0)
            VGVar.handleSysLibraryErrors = 0;
            sysLib.rollback();
            set T170UM01.T170UI-ACAO[T170UI-IND] bold;
            return;
          end
        else
          /* Inclusao*/
          T170UP-ADD-81(); /* inclui linha*/
          if (converseVar.validationMsgNum != 0)
            VGVar.handleSysLibraryErrors = 0;
            sysLib.rollback();
            set T170UM01.T170UI-ACAO[T170UI-IND] bold;
            return;
          end
        end
      end
      T170UI-ATUALIZOU = "SIM";
      VGVar.handleSysLibraryErrors = 0;
      sysLib.commit();
      T170UM01.T170UI-ACAO[T170UI-IND] = " ";
      T170UW01.T170UI-ACAO[T170UI-IND] = " ";
    end
  end
  T170UI-ATUALIZOU = "NAO";
  converseLib.validationFailed(41); /* atualizacao concluida*/
  T170UP-EXE-MONTELA(); /* monta nova tela*/
end // end T170UP-EXE-ATUAL

// Critica dos dados
Function T170UP-EXE-CRITICA()
  T170UI-IND = 0;
  while (T170UI-IND < 13)
    T170UI-IND = T170UI-IND + 1;
    if (T170UM01.T170UI-ACAO[T170UI-IND] == "A"
     or T170UM01.T170UI-ACAO[T170UI-IND] == "I")
      if (T170UM01.T170UI-SI-UF[T170UI-IND] == " ")
        converseLib.validationFailed(13); /* Campo obrigatorio*/
        set T170UM01.T170UI-SI-UF[T170UI-IND] cursor;
        return;
      end
      if (T170UM01.T170UI-NO-UF[T170UI-IND] == " ")
        converseLib.validationFailed(13); /* Campo obrigatorio*/
        set T170UM01.T170UI-NO-UF[T170UI-IND] cursor;
        return;
      end
    end
  end
  ;
  /* Posiciona cursor*/
  if (converseVar.validationMsgNum == 0)
    set T170UM01.T170UI-ACAO[1] bold, cursor;
    T170UI-IND = 0;
    while (T170UI-IND < 13)
      T170UI-IND = T170UI-IND + 1;
      if (T170UM01.T170UI-ACAO[T170UI-IND] is modified
       and T170UM01.T170UI-ACAO[T170UI-IND] != " ")
        if (T170UM01.T170UI-ACAO[T170UI-IND] == "E")
          set T170UM01.T170UI-ACAO[T170UI-IND] bold, cursor;
        else
          if (T170UM01.T170UI-ACAO[T170UI-IND] == "I")
            set T170UM01.T170UI-SI-UF[T170UI-IND] cursor;
          else
            set T170UM01.T170UI-NO-UF[T170UI-IND] cursor;
          end
        end
        T170UI-IND = 13;
      end
    end
  end
end // end T170UP-EXE-CRITICA

// Trata destino
