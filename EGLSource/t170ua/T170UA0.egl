// Critica Acao (A,E e I)
Function T170US-CRIT-ACAO()
  T170UI-IND = 0;
  while (T170UI-IND < 13)
    T170UI-IND = T170UI-IND + 1;
    if (T170UM01.T170UI-ACAO[T170UI-IND] == " "
     or T170UM01.T170UI-ACAO[T170UI-IND] == "A"
     or T170UM01.T170UI-ACAO[T170UI-IND] == "E"
     or T170UM01.T170UI-ACAO[T170UI-IND] == "I")
      if (T170UM01.T170UI-ACAO[T170UI-IND] == " " /* atual =  ' '*/
       and T170UW01.T170UI-ACAO[T170UI-IND] != " ") /* anterior ne ' '*/
  /* ..............Restaura a linha*/
        T170UM01.T170UI-SI-UF[T170UI-IND] = T170UW01.T170UI-SI-UF[T170UI-IND];
        T170UM01.T170UI-NO-UF[T170UI-IND] = T170UW01.T170UI-NO-UF[T170UI-IND];
      else
  /* ..............Restaura a acao*/
        if (T170UW01.T170UI-ACAO[T170UI-IND] != " " /* anterior ne ' '*/
         and T170UM01.T170UI-ACAO[T170UI-IND] !=    
        T170UW01.T170UI-ACAO[T170UI-IND])           /* acao atual ne anterior*/
          T170UM01.T170UI-ACAO[T170UI-IND] = T170UW01.T170UI-ACAO[T170UI-IND]; /* Volta acao anterior*/
        end
      end
  /* ;;;;;;;;;;;;;;;;;;;;*/
      if (T170UM01.T170UI-ACAO[T170UI-IND] == "A")
        if (T170UM01.T170UI-SI-UF[T170UI-IND] == " ")
          T170UM01.T170UI-ACAO[T170UI-IND] = " ";
        else
          set T170UM01.T170UI-NO-UF[T170UI-IND] normal;
        end
      else
        if (T170UM01.T170UI-ACAO[T170UI-IND] == "I")
          T170UM01.T170UI-SI-UF[T170UI-IND] = " ";
          T170UM01.T170UI-NO-UF[T170UI-IND] = " ";
          set T170UM01.T170UI-SI-UF[T170UI-IND] normal;
          set T170UM01.T170UI-NO-UF[T170UI-IND] normal;
        else
          if (T170UM01.T170UI-ACAO[T170UI-IND] == "E")
            if (T170UM01.T170UI-SI-UF[T170UI-IND] == 0)
              T170UM01.T170UI-ACAO[T170UI-IND] = " ";
            else
              set T170UM01.T170UI-SI-UF[T170UI-IND] protect, bold;
              set T170UM01.T170UI-NO-UF[T170UI-IND] protect, bold;
            end
          else
            set T170UM01.T170UI-SI-UF[T170UI-IND] protect;
            set T170UM01.T170UI-NO-UF[T170UI-IND] protect;
          end
        end
      end
   /* ;;;;;;;;;;;;;;;*/
      T170UW01.T170UI-ACAO[T170UI-IND] = T170UM01.T170UI-ACAO[T170UI-IND]; /* Guarda acao atual*/
    else
      set T170UM01.T170UI-SI-UF[T170UI-IND] protect;
      set T170UM01.T170UI-NO-UF[T170UI-IND] protect;
      if (converseVar.validationMsgNum == 0)
        converseLib.validationFailed(21); /* acao invalida;*/
        set T170UM01.T170UI-ACAO[T170UI-IND] bold, cursor, modified;
      else
        set T170UM01.T170UI-ACAO[T170UI-IND] bold, modified;
      end
    end
  end
  set T170UM01.T170UI-ACAO[1] bold, cursor;
  if (converseVar.validationMsgNum == 0)
    set T170UM01.T170UI-ACAO[1] bold, cursor;
    T170UI-IND = 0;
    while (T170UI-IND < 13)
      T170UI-IND = T170UI-IND + 1;
      if (T170UM01.T170UI-ACAO[T170UI-IND] != " ")
        set T170UM01.T170UI-ACAO[T170UI-IND] bold, cursor;
        T170UI-IND = 13;
      end
    end
  else
    if (converseVar.validationMsgNum == 21) /* acao invalida*/
      T170UI-IND = 0;
      while (T170UI-IND < 13)
        T170UI-IND = T170UI-IND + 1;
        if (T170UM01.T170UI-ACAO[T170UI-IND] != " "
         and T170UM01.T170UI-ACAO[T170UI-IND] != "A"
         and T170UM01.T170UI-ACAO[T170UI-IND] != "E"
         and T170UM01.T170UI-ACAO[T170UI-IND] != "I")
          set T170UM01.T170UI-ACAO[T170UI-IND] bold, cursor;
          T170UI-IND = 13;
        end
      end
    end
  end
end // end T170US-CRIT-ACAO

// Rotina de mapa help
Function ZZZ00S-AJUDA()
   /* ==========================================*/
   /* Rotina de chamamento do modulo de Ajuda =*/
   /* ==========================================*/
  ;
  NA000I-ERRO-CALL = 0;
  NA000I-SISTEMA77 = NA000W01.NA000I-SISTEMA;
