// Monta tela - 13 quadros
Function T170UP-EXE-MONTELA()
  set T170UM01 initial;
  move " " to T170UW01.T170UI-ACAO[1] for all;
  if (T170UW01.T170UI-TELA == 0)
    T170UW01.T170UI-TELA = 1;
  end
  T170UI-IND = T170UW01.T170UI-TELA * 13 - 12;
  T170UM01.T170UI-PAGINA = T170UW01.T170UI-TELA;
  T170UP-SEQ-81();
  T170UP-SCN-81();
  ;
  T170UI-IND2 = 1;
  /* Pula t170ui-ind linhas*/
  while (T170UI-IND2 < T170UI-IND
   and SRHXVW81_UNID_FED not noRecordFound)
    T170UP-SCN-81();
    T170UI-IND2 = T170UI-IND2 + 1;
  end
  ;
  T170UI-IND2 = 0;
  /* monta Tela 1*/
  while (T170UI-IND2 < 13
   and SRHXVW81_UNID_FED not noRecordFound)
    T170UI-IND2 = T170UI-IND2 + 1;
    T170UM01.T170UI-SI-UF[T170UI-IND2] = SRHXVW81_UNID_FED.SI_UF;
    T170UM01.T170UI-NO-UF[T170UI-IND2] = SRHXVW81_UNID_FED.NO_UF;
    T170UP-SCN-81();
  end
  ;
  /* .............. Verifica se e ultima pagina*/
  if (" " in T170UM01.T170UI-SI-UF)
    converseLib.validationFailed(24);
  end
  ;
  /* .............. Salva os dados da Tela*/
  move T170UM01.T170UI-SI-UF to T170UW01.T170UI-SI-UF[1] for all;
  move T170UM01.T170UI-NO-UF to T170UW01.T170UI-NO-UF[1] for all;
end // end T170UP-EXE-MONTELA

// CONTROLE GERAL DA APLICACAO
Function T170UP-EXE-PRINC()
  T170UP-EXE-INICIO(); /* procedimentos iniciais*/
  while (1 == 1)
    T170UP-EXE-MONTELA(); /* * MONTA TELA DE CODIGOS*/
    while (NA000W01.NA000I-PARAMETRO == " ")
      T170UP-CON-MAPA(); /* mostra tela*/
      if (converseVar.eventKey is enter)
        if ("A" in T170UM01.T170UI-ACAO
         or "E" in T170UM01.T170UI-ACAO
         or "I" in T170UM01.T170UI-ACAO)
          T170UM01.T170UI-PARAMETRO = " ";
          T170UM01.T170UI-DESTINO = " ";
          T170UP-EXE-CRITICA();
          if (converseVar.validationMsgNum == 0)
            converseLib.validationFailed(31); /* f2 e f6*/
          end
        else
          if (T170UM01.T170UI-DESTINO == " ")
            if (T170UM01.T170UI-PARAMETRO != " ")
              converseLib.validationFailed(13); /* campo obrigatorio*/
              set T170UM01.T170UI-DESTINO cursor;
            else
              converseLib.validationFailed(254); /* digite uma acao ou tecle uma <pf> ou*/
                                   /* informe parametro e/ou destino*/
            end
          else
            if (T170UM01.T170UI-PARAMETRO != " ")
              NA000W01.NA000I-PARAMETRO = T170UM01.T170UI-PARAMETRO;
            end
            NA000I-DESTINO-CALL = T170UM01.T170UI-DESTINO;
            T170UP-EXE-DESTINO();
          end
        end
      else
        if (converseVar.eventKey is pf2) /* atualizacao*/
          if ("A" in T170UM01.T170UI-ACAO
           or "E" in T170UM01.T170UI-ACAO
           or "I" in T170UM01.T170UI-ACAO)
            T170UP-EXE-ATUAL(); /* atualiza*/
          else
            converseLib.validationFailed(159); /* nada a atualizar;*/
          end
        else
          if (converseVar.eventKey is pf6) /* cancela atualizacao*/
            T170UP-EXE-MONTELA(); /* * MONTA TELA DE CODIGOS*/
          else
            if (converseVar.eventKey is pf3)
              if ("A" in T170UM01.T170UI-ACAO
               or "E" in T170UM01.T170UI-ACAO
               or "I" in T170UM01.T170UI-ACAO)
                converseLib.validationFailed(31); /* aviso de atualizacao*/
              else
                NAV00S-NAV09A0(); /* retorna ao navegador*/
              end
            else
              if (converseVar.eventKey is pf7) /* tela anterior*/
                if ("A" in T170UM01.T170UI-ACAO
                 or "E" in T170UM01.T170UI-ACAO
                 or "I" in T170UM01.T170UI-ACAO)
                  converseLib.validationFailed(31); /* aviso  atualizar;*/
                else
                  T170UP-EXE-TELAANT();
                end
              else
                if (converseVar.eventKey is pf8) /* tela posterior*/
                  if ("A" in T170UM01.T170UI-ACAO
                   or "E" in T170UM01.T170UI-ACAO
                   or "I" in T170UM01.T170UI-ACAO)
                    converseLib.validationFailed(31); /* aviso  atualizar;*/
                  else
                    T170UP-EXE-TELAPOS();
                  end
                else
                  if (converseVar.eventKey is pf5) /* cursor no parametro*/
                    if ("A" in T170UM01.T170UI-ACAO
                     or "E" in T170UM01.T170UI-ACAO
                     or "I" in T170UM01.T170UI-ACAO)
                      converseLib.validationFailed(31); /* aviso  atualizar;*/
                    else
                      set T170UM01.T170UI-PARAMETRO cursor;
                    end
                  else
                    if (converseVar.eventKey is pf1) /* ajuda*/
                      ZZZ00S-AJUDA(); /* rotina de ajuda*/
                      if (converseVar.validationMsgNum == 0)
                        converseLib.validationFailed(31); /* aviso  atualizar;*/
                      end
                    else
                      if (converseVar.eventKey is pf4) /* menu*/
                        if ("A" in T170UM01.T170UI-ACAO
                         or "E" in T170UM01.T170UI-ACAO
                         or "I" in T170UM01.T170UI-ACAO)
                          converseLib.validationFailed(31); /* aviso  atualizar;*/
                        else
                          NA000I-DESTINO-CALL = "MENU";
                          T170UP-EXE-DESTINO();
                        end
                      else
                        if (converseVar.eventKey is pf12)
                          if ("A" in T170UM01.T170UI-ACAO
                           or "E" in T170UM01.T170UI-ACAO
                           or "I" in T170UM01.T170UI-ACAO)
                            converseLib.validationFailed(31); /* aviso  atualizar;*/
                          else
                            NA000I-DESTINO-CALL = "FIM";
                            T170UP-EXE-DESTINO();
                          end
                        else
                          converseLib.validationFailed(7); /* tecla invalida*/
                        end
                      end
                    end
                  end
                end
              end
            end
          end
        end
      end
    end
  end /* while 1=1*/
end // end T170UP-EXE-PRINC

// Trata PF7
Function T170UP-EXE-TELAANT()
  if (T170UW01.T170UI-TELA == 1)
    converseLib.validationFailed(23); /* inicio da lista;*/
  else
    T170UW01.T170UI-TELA = T170UW01.T170UI-TELA - 1;
    if (T170UW01.T170UI-TELA <= 1)
      converseLib.validationFailed(23); /* inicio da lista;*/
    end
    T170UP-EXE-MONTELA();
  end
end // end T170UP-EXE-TELAANT

// Trata PF8
Function T170UP-EXE-TELAPOS()
  if (T170UM01.T170UI-SI-UF[13] == " ")
    converseLib.validationFailed(24); /* fim da lista;*/
  else
    T170UW01.T170UI-TELA = T170UW01.T170UI-TELA + 1;
    T170UP-EXE-MONTELA();
    if (T170UM01.T170UI-SI-UF[1] == " ")
      T170UW01.T170UI-TELA = T170UW01.T170UI-TELA - 1;
      T170UP-EXE-MONTELA();
      converseLib.validationFailed(24); /* fim da lista;*/
    end
  end
end // end T170UP-EXE-TELAPOS

